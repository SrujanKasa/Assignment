      "cell_type": "code",
      "source": [
        "# -------------------------------------------------------------\n",
        "#  IMPORTS\n",
        "# -------------------------------------------------------------\n",
        "import tensorflow as tf\n",
        "from keras.datasets import mnist\n",
        "from keras.models import Sequential, load_model\n",
        "from keras.layers import Flatten, Dense\n",
        "from keras.preprocessing.image import load_img, img_to_array\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from google.colab import files\n",
        "import os\n",
        "from google.colab import drive\n",
        "drive.mount('/content/drive')\n",
        "\n",
        "# If you uploaded 'project.h5' using files.upload(), it's in the uploaded dict\n",
        "if 'project.h5' in uploaded:\n",
        "    # write bytes to disk temporarily then load\n",
        "    with open('/content/project.h5', 'wb') as f:\n",
        "        f.write(uploaded['project.h5'])\n",
        "    model = load_model('/content/project.h5')\n",
        "    print(\"Loaded model from uploaded file 'project.h5'.\")\n",
        "elif os.path.exists('/content/drive/MyDrive/ML_models/project.h5'):\n",
        "    model = load_model('/content/drive/MyDrive/ML_models/project.h5')\n",
        "    print(\"Loaded model from Drive: /content/drive/MyDrive/ML_models/project.h5\")\n",
        "else:\n",
        "    print(\"No 'project.h5' found in upload or Drive. Place your model at one of those locations.\")\n",
        "# -------------------------------------------------------------\n",
        "#  LOAD MNIST DATA\n",
        "# -------------------------------------------------------------\n",
        "(train_img, train_lab), (test_img, test_lab) = mnist.load_data()\n",
        "\n",
        "train_img = train_img.astype('float32')/255.0\n",
        "test_img = test_img.astype('float32')/255.0\n",
        "\n",
        "print(\"Training shape:\", train_img.shape)\n",
        "# -------------------------------------------------------------\n",
        "#  BUILD MODEL\n",
        "# -------------------------------------------------------------\n",
        "model = Sequential([\n",
        "    Flatten(input_shape=(28,28)),\n",
        "    Dense(512, activation='relu'),\n",
        "    Dense(256, activation='relu'),\n",
        "    Dense(10, activation='softmax')\n",
        "])\n",
        "\n",
        "model.compile(optimizer='adam',\n",
        "              loss='sparse_categorical_crossentropy',\n",
        "              metrics=['accuracy'])\n",
        "\n",
        "model.summary()\n",
        "# -------------------------------------------------------------\n",
        "#  TRAIN MODEL\n",
        "# -------------------------------------------------------------\n",
        "model.fit(train_img, train_lab, epochs=5, batch_size=32, validation_split=0.1)\n",
        "\n",
        "loss, acc = model.evaluate(test_img, test_lab)\n",
        "print(\"Test Accuracy:\", acc)\n",
        "# -------------------------------------------------------------\n",
        "#  SAVE MODEL (OPTIONAL)\n",
        "# -------------------------------------------------------------\n",
        "model.save('/content/drive/MyDrive/ML_models/project.h5')\n",
        "print(\"Model saved as project.h5\")\n",
        "# -------------------------------------------------------------\n",
        "#  PREPROCESS THE IMAGE\n",
        "# -------------------------------------------------------------\n",
        "def prepare_image(path):\n",
        "    # Load as grayscale, resize to MNIST size\n",
        "    img = load_img(path, color_mode='grayscale', target_size=(28,28))\n",
        "\n",
        "    # Convert to array\n",
        "    img = img_to_array(img)\n",
        "\n",
        "    # Reshape to (1,28,28)\n",
        "    img = img.reshape(1,28,28)\n",
        "\n",
        "    # Normalize\n",
        "    img = img.astype('float32') / 255.0\n",
        "\n",
        "    return img\n",
        "# === CELL 5 - Read an uploaded image and display it ===\n",
        "# Finds any common image file among uploaded files and displays it\n",
        "img_fname = None\n",
        "for fname in uploaded:\n",
        "    if fname.lower().endswith(('.png', '.jpg', '.jpeg', '.bmp', '.gif')):\n",
        "        img_fname = fname\n",
        "        break\n",
        "\n",
        "if img_fname:\n",
        "    img_display = Image.open(BytesIO(uploaded[img_fname])).convert('L')  # convert to grayscale\n",
        "    print(\"Image:\", img_fname, \"size:\", img_display.size, \"mode:\", img_display.mode)\n",
        "    plt.figure(figsize=(4,4))\n",
        "    plt.imshow(img_display, cmap='gray')\n",
        "    plt.axis('off')\n",
        "    plt.show()\n",
        "\n",
        "    # Save the uploaded image bytes to a temporary file for processing\n",
        "    temp_img_path = '/tmp/' + img_fname\n",
        "    with open(temp_img_path, 'wb') as f:\n",
        "        f.write(uploaded[img_fname])\n",
        "\n",
        "    # Prepare the image for prediction using the temporary file path\n",
        "    img_for_prediction = prepare_image(temp_img_path)\n",
        "\n",
        "    plt.figure(figsize=(4,4)) # Ensure consistent display size for clarity\n",
        "    plt.imshow(img_for_prediction.reshape(28,28), cmap='gray')\n",
        "    plt.title(\"Your uploaded image (preprocessed for model)\")\n",
        "    plt.axis('off')\n",
        "    plt.show()\n",
        "\n",
        "    # -------------------------------------------------------------\n",
        "    #  RUN PREDICTION\n",
        "    # -------------------------------------------------------------\n",
        "    pred = model.predict(img_for_prediction)\n",
        "    digit = np.argmax(pred[0])\n",
        "\n",
        "    print(\"Predicted Digit:\", digit)\n",
        "\n",
        "    # Clean up the temporary file\n",
        "    os.remove(temp_img_path)\n",
        "\n",
        "else:\n",
        "    print(\"No image file found among uploaded files.\")\n"
      ],
